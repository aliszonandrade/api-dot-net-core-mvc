@model ClienteModel

@{ 
    int id = 0;
    string nome = string.Empty;
    string datacadastro = string.Empty;
    string cpfcnpj = string.Empty;
    string tipo = string.Empty;
    string telefone = string.Empty;
    string email = string.Empty;
    string cep = string.Empty;
    string logradouro = string.Empty;
    string numero = string.Empty;
    string bairro = string.Empty;
    string complemento = string.Empty;
    string cidade = string.Empty;
    string uf = string.Empty;
    string datanascimento = string.Empty;

    try
    {
        nome = ViewBag.Registro.Nome.ToString();
        datacadastro = ViewBag.Registro.DataCadastro.ToString();
        cpfcnpj = ViewBag.Registro.CpfCnpj.ToString();
        tipo = ViewBag.Registro.Tipo.ToString();
        telefone = ViewBag.Registro.Telefone.ToString();
        email = ViewBag.Registro.Email.ToString();
        cep = ViewBag.Registro.CEP.ToString();
        logradouro = ViewBag.Registro.Logradouro.ToString();
        numero = ViewBag.Registro.Numero.ToString();
        bairro = ViewBag.Registro.Bairro.ToString();
        complemento = ViewBag.Registro.Complemento.ToString();
        cidade = ViewBag.Registro.Cidade.ToString();
        uf = ViewBag.Registro.UF.ToString();
        datanascimento = ViewBag.Registro.DataNascimento.ToString();
    }
    catch
    {

    }
}
<h1>Registrar cliente</h1>
<form asp-controller="Home" asp-action="Registrar" role="form">
    <div class="row">
        <div class="col-lg-3">
            <label>Nome:</label>
            <input asp-for="Nome" value="@nome" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>Data cadastro:</label>
            <input asp-for="DataCadastro" value="@datacadastro" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>CPF/CNPJ:</label>
            <input asp-for="CpfCnpj" value="@cpfcnpj" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>Tipo:</label>
            <input asp-for="Tipo" value="@tipo" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>Telefone:</label>
            <input asp-for="Telefone" value="@telefone" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>E-mail:</label>
            <input asp-for="Email" value="@email" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>CEP:</label>
            <input asp-for="CEP" value="@cep" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>Logradouro:</label>
            <input asp-for="Logradouro" value="@logradouro" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>Número:</label>
            <input asp-for="Numero" value="@numero" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>Bairro:</label>
            <input asp-for="Bairro" value="@bairro" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>Complemento:</label>
            <input asp-for="Complemento" value="@complemento" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>Cidade:</label>
            <input asp-for="Cidade" value="@cidade" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>UF:</label>
            <input asp-for="UF" value="@uf" class="form-control" />
        </div>
        <div class="col-lg-3">
            <label>Data de nascimento:</label>
            <input asp-for="DataNascimento" value="@datanascimento" class="form-control" />
        </div>
    </div>
    <br />
    <button type="submit" class="btn btn-block btn-success">Registrar</button>
    <button type="reset" class="btn btn-block btn-danger">Limpar</button>
</form>